<?php

	class Overloading // przeciazenie
	{
		public function Operacja($cokolwiek = null, $nastepne = null)
		{
			$args = func_get_args();
			
			var_dump($args);
		}
		
		
	}
	
	$overl = new Overloading();
	
	$overl->Operacja();
	
	$overl->Operacja("dfsa");
	
	$overl->Operacja("dsaffdas", "fdsas");
@{
    ViewBag.Title = "_DysqLayout";
}

<!--<h2>_DysqLayout</h2>-->
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" type="text/css" href="~/Areas/Dysq/Content/style/Dysq_style.css" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" type="text/css" />


</head>
    <body>
        <div class ="container-fluid">
            <!--gora-->
            <div id="row-top" class="row">
                <div class="col-xs-2  col-xs-offset-1 col-sm-2 col-sm-offset-1 col-md-2 col-md-offset-1 col-lg-2 col-lg-offset-1">
                    <img src="http://p2.galapp.net/OP/0006/Web/aebf8e2b/Grafika/logo.png" class="img-responsive"/>
                    <img src="http://p2.galapp.net/OP/0006/Web/aebf8e2b/Grafika/para.png" class="img-responsive"/>
                     </div>
                <div id="data-jezyk" class="col-xs-8 col-sm-8 col-md-8 col-lg-8">
                    <div class="col-xs-4 col-sm-4 col-md-4 col-lg-4">Dzisiaj jest: <b>@ViewBag.CurrentTime</b></div>
                    <div id="jezyk" class="col-xs-3 col-sm-3 col-md-3 col-lg-3">Wybierz język:
                        <span id="flagi-jezyk">
                            <img src="~/Areas/Dysq/Content/image/pl.png"/>
                            <img src="~/Areas/Dysq/Content/image/de.png"/>
                            <img src="~/Areas/Dysq/Content/image/eng.png"/>
                        </span>
                    </div>
                </div>
                
                <div class="col-xs-7 col-sm-7 col-md-7 col-lg-7">
                    <div  class="row">
                        <div id="nested-col-mieszkania" class="col-xs-2 col-sm-2 col-md-2 col-lg-2">
                            <p id="menu-oferta">Mieszkania</p>
                        </div>
                        <div id="nested-col-domy" class="col-xs-2 col-sm-2 col-md-2 col-lg-2">
                            <p id="menu-oferta">Domy</p>
                        </div>
                        <div id="nested-col-dzialki" class="col-xs-2 col-sm-2 col-md-2 col-lg-2">
                            <p id="menu-oferta">Dzialki</p>
                        </div>
                        <div id="nested-col-komercyjne" class="col-xs-2 col-sm-2 col-md-2 col-lg-2">
                            <p id="menu-oferta">Komercyjne</p>
                        </div>
                        <div id="nested-col-deweloperskie" class="col-xs-2 col-sm-2 col-md-2 col-lg-2">
                            <p id="menu-oferta">Deweloperskie</p>
                        </div>
                    </div>

                </div>


               
    
            </div>
            
        </div>
           
        @RenderBody()
    </body>
    </html>
